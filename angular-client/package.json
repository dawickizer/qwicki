    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/router": "^14.2.0",
    "@babylonjs/core": "^5.33.2",
    "@babylonjs/gui": "^5.33.2",
    "@babylonjs/inspector": "^5.33.2",
